  file = {C\:\\Users\\user\\Zotero\\storage\\QR7V84JC\\Krull et al_2003_Importance of mechanisms and processes of the stabilisation of soil organic.pdf;C\:\\Users\\user\\Zotero\\storage\\JZR65YQZ\\FP02085.html}
}

@article{kurzCBMCFS3ModelCarbondynamics2009,
