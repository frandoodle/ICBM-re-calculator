@article{smithCarbonSequestrationPotential2005,
  title = {Carbon Sequestration Potential in {{European}} Croplands Has Been Overestimated},
  author = {Smith, Pete and Andren, Olof and Karlsson, Thord and Perala, Paula and Regina, Kristiina and Rounsevell, Mark and Wesemael, Bas},
