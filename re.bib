@article{mohantyCarbonNitrogenMineralization2013,
  title = {Carbon and Nitrogen Mineralization Kinetics in Soil of Rice–Rice System under Long Term Application of Chemical Fertilizers and Farmyard Manure},
  author = {Mohanty, Sangita and Nayak, A.K. and Kumar, Anjani and Tripathi, Rahul and Shahid, Mohammad and Bhattacharyya, P. and Raja, R. and Panda, B.B.},
