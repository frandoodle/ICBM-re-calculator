@article{weiActiveCarbonPoolsize2020,
  title = {Active Carbon Pool-Size Is Enhanced by Long-Term Manure Application},
  author = {Wei, Meng and Zhang, Aijun and Tang, Zhonghou and Zhao, Peng and Pan, Hong and Wang, Hui and Yang, Quangang and Lou, Yanhong and Zhuge, Yuping},
  date = {2020-11-02},
  journaltitle = {Plant, Soil and Environment},
  shortjournal = {Plant Soil Environ.},
  volume = {66},
