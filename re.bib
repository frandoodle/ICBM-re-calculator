@article{wangSolidCattleManure2018,
  title = {Solid {{Cattle Manure Less Prone}} to {{Phosphorus Loss}} in {{Tile Drainage Water}}},
  author = {Wang, Y. T. and Zhang, T. Q. and Tan, C. S. and Qi, Z. M. and Welacky, T.},
