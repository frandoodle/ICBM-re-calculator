  title = {Regional Simulation of Soil Organic Carbon Dynamics for Dry Farmland in {{Northeast China}} Using the {{CENTURY}} Model},
  author = {Zhang, Feng and Wang, Shihang and Zhao, Mingsong and Qin, Falv and Liu, Xiaoyu},
