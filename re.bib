@article{bernalCarbonMineralizationOrganic1998,
  title = {Carbon Mineralization from Organic Wastes at Different Composting Stages during Their Incubation with Soil},
  author = {Bernal, M.P. and Sánchez-Monedero, M.A. and Paredes, C. and Roig, A.},
  date = {1998-07},
