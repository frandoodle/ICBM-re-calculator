@article{zhangImprovedCropCanopy2018,
  title = {Improved {{Crop Canopy}} and {{Water Balance Dynamics}} for {{Agroecosystem Modeling Using DayCent}}},
  author = {Zhang, Yao and Suyker, Andrew and Paustian, Keith},
  date = {2018-03},
  journaltitle = {Agronomy Journal},
  shortjournal = {Agron. J.},
  volume = {110},
