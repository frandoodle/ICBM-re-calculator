@article{risbergComparativeCharacterizationDigestate2017,
  title = {Comparative Characterization of Digestate versus Pig Slurry and Cow Manure – {{Chemical}} Composition and Effects on Soil Microbial Activity},
  author = {Risberg, Kajsa and Cederlund, Harald and Pell, Mikael and Arthurson, Veronica and Schnürer, Anna},
  date = {2017-03},
  journaltitle = {Waste Management},
