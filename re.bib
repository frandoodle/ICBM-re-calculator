@article{gregorichChangesSoilCarbon2001,
  title = {Changes in Soil Carbon under Long-Term Maize in Monoculture and Legume-Based Rotation},
  author = {Gregorich, E G and Drury, C F and Baldock, J A},
  date = {2001-02-01},
  journaltitle = {Canadian Journal of Soil Science},
