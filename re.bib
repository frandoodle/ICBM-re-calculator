@article{liangManureinducedCarbonRetention2021,
  title = {Manure-Induced Carbon Retention Measured from Long-Term Field Studies in {{Canada}}},
  author = {Liang, Chang and Hao, Xiying and Schoenau, Jeff and Ma, Bao-Luo and Zhang, Tiequan and MacDonald, J. Douglas and Chantigny, Martin and Dyck, Miles and Smith, W.N. and Malhi, Sukhdev S. and Thiagarajan, Arumugam and Lafond, Jean and Angers, Denis},
