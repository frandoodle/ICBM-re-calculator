  author = {Daggupati, P. and Pai, N. and Ale, S. and Douglas-Mankin, K. R. and Zeckoski, R. W. and Jeong, J. and Parajuli, P. B. and Saraswat, D. and Youssef, M. A.},
  date = {2015-12-30},
  journaltitle = {Transactions of the ASABE},
  shortjournal = {Trans. ASABE},
