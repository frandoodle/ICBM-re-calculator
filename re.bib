@article{sleutelEffectManureFertilizer2006,
  title = {Effect of Manure and Fertilizer Application on the Distribution of Organic Carbon in Different Soil Fractions in Long-Term Field Experiments},
  author = {Sleutel, Steven and De Neve, Stefaan and Németh, Tamás and Tóth, Tibor and Hofman, Georges},
  date = {2006-10},
