@article{lynchManagementCanadianPrairie2005,
  title = {Management of {{Canadian}} Prairie Region Grazed Grasslands: {{Soil C}} Sequestration, Livestock Productivity and Profitability},
  shorttitle = {Management of {{Canadian}} Prairie Region Grazed Grasslands},
  author = {Lynch, D. H. and Cohen, R. D. H. and Fredeen, A. and Patterson, G. and Martin, R. C.},
  date = {2005-05-01},
  journaltitle = {Canadian Journal of Soil Science},
  shortjournal = {Can. J. Soil. Sci.},
  volume = {85},
  number = {2},
