  shorttitle = {Potential for Carbon Sequestration in {{European}} Soils},
  author = {Smith, Pete and Powlson, David and Glendining, Margaret and Smith, Jo},
  date = {1997-02},
  journaltitle = {Global Change Biology},
  volume = {3},
